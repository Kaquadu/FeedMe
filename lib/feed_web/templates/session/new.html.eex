<%= form_for @changeset, Routes.session_path(@conn, :create), [multiplart: true, class: "main-form"], fn f -> %>
  <div class="form-row">
    <%= label f, :email %>
    <%= text_input f, :email %>
    <%= error_tag f, :email %>
  </div>

  <div class="form-row">
    <%= label f, :password %>
    <%= password_input f, :password %>
    <%= error_tag f, :password %>
  </div>

  <%= submit "Submit", class: "submit-button" %>
<% end %>

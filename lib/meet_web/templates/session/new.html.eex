<%= form_for @changeset, Routes.session_path(@conn, :create), [multiplart: true], fn f -> %>
  <%= label f, :email %>
  <%= text_input f, :email %>
  <%= error_tag f, :email %>

  <%= label f, :password %>
  <%= password_input f, :password %>
  <%= error_tag f, :password %>

  <%= submit "Submit" %>
<% end %>
